{
  "name": "@material-solid/utils",
  "version": "0.0.1",
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "deminearchiver",
    "url": "https://github.com/deminearchiver"
  },
  "keywords": [
    "solid",
    "solidjs",
    "components",
    "headless",
    "primitives"
  ],
  "license": "MIT",
  "homepage": "https://material-solid.pages.dev",
  "bugs": {
    "url": "https://github.com/deminearchiver/material-solid/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deminearchiver/material-solid.git",
    "directory": "packages/components"
  },
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "type": "module",
  "exports": {
    "./clsx": {
      "import": "./dist/clsx/index.js",
      "require": "./dist/clsx/index.cjs",
      "types": "./src/clsx/index.ts"
    },
    "./floating": {
      "import": "./dist/floating/index.js",
      "require": "./dist/floating/index.cjs",
      "types": "./src/floating/index.ts"
    },
    "./math": {
      "import": "./dist/math/index.js",
      "require": "./dist/math/index.cjs",
      "types": "./src/math/index.ts"
    },
    "./multi-provider": {
      "import": "./dist/multi-provider/index.js",
      "require": "./dist/multi-provider/index.cjs",
      "types": "./src/multi-provider/index.ts"
    },
    "./portal": {
      "import": "./dist/portal/index.js",
      "require": "./dist/portal/index.cjs",
      "types": "./src/portal/index.ts"
    },
    "./presence": {
      "import": "./dist/presence/index.js",
      "require": "./dist/presence/index.cjs",
      "types": "./src/presence/index.ts"
    },
    "./refs": {
      "import": "./dist/refs/index.js",
      "require": "./dist/refs/index.cjs",
      "types": "./src/refs/index.ts"
    },
    "./types": {
      "import": "./dist/types/index.js",
      "require": "./dist/types/index.cjs",
      "types": "./src/types/index.ts"
    },
    "./vanilla-extract": {
      "import": "./dist/vanilla-extract/index.js",
      "require": "./dist/vanilla-extract/index.cjs",
      "types": "./src/vanilla-extract/index.ts"
    }
  },
  "packageManager": "yarn@4.3.1",
  "scripts": {
    "clean": "rimraf node_modules dist",
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@solid-primitives/context": "^0.2.3",
    "@solid-primitives/map": "^0.4.11",
    "@solid-primitives/presence": "^0.0.6",
    "@solid-primitives/promise": "^1.0.17",
    "@solid-primitives/refs": "^1.0.8",
    "@solid-primitives/set": "^0.4.11",
    "@solid-primitives/static-store": "^0.0.8",
    "@solid-primitives/utils": "^6.2.3",
    "clsx": "^2.1.1",
    "scule": "^1.3.0",
    "solid-js": "^1.8.19"
  },
  "devDependencies": {
    "@material-solid/config": "workspace:^",
    "@material-solid/scripts": "workspace:^",
    "@solidjs/testing-library": "^0.8.9",
    "@types/node": "^22.0.2",
    "@vitest/browser": "^2.0.5",
    "@vitest/coverage-v8": "^2.0.5",
    "esbuild-plugin-solid": "^0.6.0",
    "fdir": "^6.2.0",
    "pkg-types": "^1.1.3",
    "rimraf": "^6.0.1",
    "tsup": "^8.2.3",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  }
}
